package com.objects;

public class student {
 int rollno;
 String name;
 String school;
 
public int getRollno() {
	return rollno;
}
public void setRollno(int rollno) {
	this.rollno = rollno;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getSchool() {
	return school;
}
public void setSchool(String school) {
	this.school = school;
}
 student(){
	 
 }
 student(int rollno,String name,String school){
	 this();
	 this.rollno=rollno;
	 this.name=name;
	 this.school=school;
 }
}
package com.objects;

public class studentcontinue {
	
