package com.inheritance;

public class parent {
	public int a=10;
	public String name="Sushmitha P";
	
	public void sum(int a,int b) {
		System.out.println("sum is: "+(a+b));
	}

}
package com.inheritance;

public class child extends parent {
    public int c=20;
    public String location="Shimoga";
    
    public void sub(int a,int b) {
    	System.out.println("substraction is: " +(a-b));
    }
    
}
package com.inheritance;

public class resultsofinheritance {
 public static void main(String[] args) {
	 parent parents=new parent();
	 child childs=new child();
	 System.out.println(parents.a);
	 System.out.println(parents.name);
	 parents.sum(50,60);
	 System.out.println(childs.a);
	 System.out.println(childs.location);
	 System.out.println(childs.name);
	 System.out.println(childs.c);
	 childs.sub(50,70);

 }
}
